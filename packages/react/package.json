{
  "name": "@deepnoodle/xray-react",
  "version": "0.1.3",
  "description": "React bindings for xray - give your AI agent eyes into your React app's runtime state",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./noop": {
      "types": "./dist/noop.d.ts",
      "import": "./dist/noop.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@deepnoodle/xray-core": "^0.1.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "14.3.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "18.3.7",
    "react": "^18.2.0",
    "react-dom": "18.3.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "react",
    "xray",
    "debugging",
    "ai",
    "agent",
    "devtools",
    "inspector"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deepnoodle-ai/xray.git",
    "directory": "packages/react"
  },
  "publishConfig": {
    "access": "public"
  }
}
