{
  "name": "@deepnoodle/xray-svelte",
  "version": "0.1.5",
  "description": "Svelte bindings for xray - give your AI agent eyes into your Svelte app's runtime state",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./noop": {
      "types": "./dist/noop.d.ts",
      "import": "./dist/noop.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@deepnoodle/xray-core": "^0.1.5"
  },
  "peerDependencies": {
    "svelte": ">=4.0.0"
  },
  "devDependencies": {
    "@testing-library/svelte": "5.3.1",
    "svelte": "^4.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "svelte",
    "xray",
    "debugging",
    "ai",
    "agent",
    "devtools",
    "inspector"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deepnoodle-ai/xray.git",
    "directory": "packages/svelte"
  },
  "publishConfig": {
    "access": "public"
  }
}
