{
  "name": "@deepnoodle/xray-core",
  "version": "0.1.3",
  "description": "Core utilities for xray - give your AI agent eyes into your app's runtime state",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js"
    },
    "./noop": {
      "types": "./dist/noop.d.ts",
      "import": "./dist/noop.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "xray",
    "debugging",
    "ai",
    "agent",
    "devtools",
    "inspector",
    "vite"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deepnoodle-ai/xray.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  }
}
